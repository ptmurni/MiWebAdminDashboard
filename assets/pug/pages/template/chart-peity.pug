- var page_Title = 'Peity Chart';
- var theme_customizer      = true;
- var peitychart            = true;
- var page_peitychart       = true;
- var sidebar               = true;
- var slick                 = true;
- var header_slider         = true;
- var animate               = true;
- var scrollbar             = true;
doctype html
html(lang='en')
  include ../../components/header-files
  body
    include ../../components/loader
    include ../../components/taptop
    // page-wrapper Start
    #pageWrapper.page-wrapper.compact-wrapper
      include ../../components/header
      // Page Body Start
      .page-body-wrapper
        include ../../components/sidebar
        .page-body
          .container.w-full
            .page-title
              .grid.grid-cols-12.mx-2.items-center
                div(class="col-span-6 sm:col-span-12")
                  h3
                    | Peity Chart
                div(class="col-span-6 sm:col-span-12")
                  ol.breadcrumb.flex.gap-2
                    li.breadcrumb-item
                      a(href='index.html')                                        
                        svg.stroke-icon
                          use(href='../assets/svg/icon-sprite.svg#stroke-home')
                    li.breadcrumb-item Charts
                    li.breadcrumb-item.active Peity Chart
          // Container-fluid starts
          .container
            .grid.grid-cols-12.card-gap
              div(class="col-span-4 xl:col-span-12")
                .card
                  .card-header
                    h5 Updating Chart
                  .card-body.peity-charts.peity-chart-primary
                    span.updating-chart 4,3,5,4,5,10,2,3,9,1,2,8
              div(class="col-span-4 xl:col-span-6 md:col-span-12")
                .card
                  .card-header
                    h5 Line Chart 1
                  .card-body.peity-charts.peity-chart-primary.rounded-md.shadow-md.bg-gray-100
                    span.line.hidden 5,3,9,6,5,9,7,3,5,2
              div(class="col-span-4 xl:col-span-6 md:col-span-12")
                .card
                  .card-header
                    h5 Line Chart 2
                  .card-body.peity-charts.peity-chart-primary
                    span.line 0,-3,-6,-4,-5,-4,-7,-3,-5,-2
              div(class="col-span-4 xl:col-span-6 md:col-span-12 box-col-6")
                .card
                  .card-header
                    h5 Bar Chart 1
                  .card-body.peity-charts.bar-odd-even
                    span.bar 5,3,9,6,5,9,7,3,5,2
              div(class="col-span-4 xl:col-span-6 md:col-span-12 box-col-6")
                .card
                  .card-header
                    h5 Bar Chart 2
                  .card-body.peity-charts.bar-odd-even
                    span.bar 5,3,2,-1,-3,-2,2,3,5,2
              div(class="col-span-4 xl:col-span-6 md:col-span-12 box-col-6")
                .card
                  .card-header
                    h5 Donut Chart 1
                  .card-body.peity-charts.donut-peity
                    span.donut 226/360
              div(class="col-span-4 xl:col-span-6 md:col-span-12 box-col-6")
                .card
                  .card-header
                    h5 Donut Chart 2
                  .card-body.peity-charts.donut-peity2
                    span.donut 1,2,3,2,2
              div(class="col-span-4 xl:col-span-6 md:col-span-12 box-col-6")
                .card
                  .card-header
                    h5 Pie Chart 1
                  .card-body.peity-charts.pie-peity
                    span.pie 226,134
              div(class="col-span-4 xl:col-span-6 md:col-span-12 box-col-6")
                .card
                  .card-header
                    h5 Pie Chart 2
                  .card-body.peity-charts.pie-peity
                    span.pie 1,2,3,2,2
              div(class="col-span-6 xxl:col-span-12 box-col-12")
                .card
                  .card-header
                    h5 Donut Charts with Radius
                  .card-body
                    p.data-attributes.flex.items-end
                      span.small-donut(data-peity='{"innerRadius": 10, "radius": 40 }') 1/7
                      span.small-donut2(data-peity='{"innerRadius": 14, "radius": 36 }') 2/7
                      span.small-donut3(data-peity='{"innerRadius": 16, "radius": 32 }') 3/7
                      span.small-donut4(data-peity='{"innerRadius": 18, "radius": 28 }') 4/7
                      span.small-donut5(data-peity='{"innerRadius": 20, "radius": 24 }') 5/7
                      span.small-donut6(data-peity='{"innerRadius": 18, "radius": 20 }') 6/7
              div(class="col-span-6 xxl:col-span-12 box-col-12")
                .card
                  .card-header
                    h5 Peity Charts With Different Colors
                  .card-body
                    .grid.grid-cols-12.card-gap
                      div(class="col-span-3 sm:col-span6 text-center")
                        .xm-mb-peity
                          span.bar-colours-1 5,3,9,6,5,9,7,3,5,2
                      div(class="col-span-3 sm:col-span6 text-center")
                        .xm-mb-peity
                          span.bar-colours-2 5,3,2,-1,-3,-2,2,3,5,2
                      div(class="col-span-3 sm:col-span6 text-center")
                        span.bar-colours-3 0,-3,-6,-4,-5,-4,-7,-3,-5,-2
                      div(class="col-span-3 sm:col-span6 text-center")
                        span.pie-colours-1 4,7,6,5
          // Container-fluid Ends
        include ../../components/footer
    include ../../components/footer-files
