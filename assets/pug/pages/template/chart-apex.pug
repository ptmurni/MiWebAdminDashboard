- var page_Title = 'Apex Chart';
- var theme_customizer      = true;
- var apex                  = true;
- var page_apex             = true;
- var apexmoment            = true;
- var sidebar               = true;
- var slick                 = true;
- var header_slider         = true;
- var scrollbar             = true;
doctype html
html(lang='en')
  include ../../components/header-files
  body
    include ../../components/loader
    include ../../components/taptop
    // page-wrapper Start
    #pageWrapper.page-wrapper.compact-wrapper
      include ../../components/header
      // Page Body Start
      .page-body-wrapper
        include ../../components/sidebar
        .page-body
          .container.w-full
            .page-title
              .grid.grid-cols-12.mx-2.items-center
                div(class="col-span-6 sm:col-span-12")
                  h3
                    | Apex Chart
                div(class="col-span-6 sm:col-span-12")
                  ol.breadcrumb.flex.gap-2
                    li.breadcrumb-item
                      a(href='index.html')                                        
                        svg.stroke-icon
                          use(href='../assets/svg/icon-sprite.svg#stroke-home')
                    li.breadcrumb-item Charts
                    li.breadcrumb-item.active Apex Chart
          // Container-fluid starts
          .container
            .grid.grid-cols-12.card-gap
              div(class="col-span-6 xl:col-span-12 box-col-6")
                .card
                  .card-header
                    h5
                      | Basic Area Chart 
                  .card-body
                    #basic-apex
              div(class="col-span-6 xl:col-span-12 box-col-6")
                .card
                  .card-header
                    h5
                      | Area Spaline Chart 
                  .card-body
                    #area-spaline
              div(class="col-span-6 xl:col-span-12 box-col-6")
                .card
                  .card-header
                    h5
                      | Bar chart
                  .card-body
                    #basic-bar
              div(class="col-span-6 xl:col-span-12 box-col-6")
                .card
                  .card-header
                    h5
                      | Column Chart 
                  .card-body
                    #column-chart
              div(class="col-span-6 xl:col-span-12 box-col-12")
                .card
                  .card-header
                    h5 
                      | 3d Bubble Chart 
                  .card-body
                    #chart-bubble

              div(class="col-span-6 xl:col-span-12 box-col-6")
                .card
                  .card-header
                    h5
                      | Stepline Chart 
                  .card-body
                    #stepline
              div(class="col-span-12 box-col-6")
                .card
                  .card-header
                    h5
                      | Column Chart
                  .card-body
                    #annotationchart
              div(class="col-span-6 xl:col-span-12 box-col-6")
                .card
                  .card-header
                    h5
                      | Pie Chart 
                  .card-body.apex-chart
                    #piechart
              div(class="col-span-6 xl:col-span-12 box-col-6")
                .card
                  .card-header
                    h5
                      | Donut Chart
                  .card-body.apex-chart
                    #donutchart
              div(class="col-span-12 box-col-12")
                .card
                  .card-header
                    h5 Mixed Chart
                  .card-body
                    #mixedchart                        
              div(class="col-span-12 box-col-12")
                .card
                  .card-header
                    h5
                      | Candlestick Chart 
                  .card-body
                    #candlestick
              div(class="col-span-6 xl:col-span-12 box-col-6")
                .card
                  .card-header
                    h5
                      | Radar Chart 
                  .card-body
                    #radarchart
              div(class="col-span-6 xl:col-span-12 box-col-6")
                .card
                  .card-header
                    h5
                      | Radial Bar Chart
                  .card-body
                    #circlechart
          // Container-fluid Ends
        include ../../components/footer
    include ../../components/footer-files
