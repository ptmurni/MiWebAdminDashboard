- var page_Title = 'Pricing';
- var theme_customizer      = true;
- var sidebar               = true;
- var slick                 = true;
- var header_slider         = true;
- var scrollbar             = true;
doctype html
html(lang='en')
  include ../../components/header-files
  body
    include ../../components/loader
    include ../../components/taptop
    // page-wrapper Start
    #pageWrapper.page-wrapper.compact-wrapper
      include ../../components/header
      // Page Body Start
      .page-body-wrapper
        include ../../components/sidebar
        .page-body
          .container.w-full
            .page-title
              .grid.grid-cols-12.mx-2.items-center
                div(class="col-span-6 sm:col-span-12")
                  h3
                    | Pricing
                div(class="col-span-6 sm:col-span-12")
                  ol.breadcrumb.flex.gap-2
                    li.breadcrumb-item
                      a(href='index.html')                                        
                        svg.stroke-icon
                          use(href='../assets/svg/icon-sprite.svg#stroke-home')
                    li.breadcrumb-item Pages
                    li.breadcrumb-item.active Pricing
          // Container-fluid starts
          .container
            .grid.grid-cols-12
              div(class="col-span-12")
                .card
                  .card-header 
                    h5 Become a member
                  .card-body  
                    .grid.grid-cols-12.gap-4.pricing-block
                      div(class="col-span-3 lg:col-span-6 sm:col-span-12")
                        .pricingtable
                          .pricingtable-header
                            h3.title Standard
                          .price-value
                            span.currency $
                            span.amount 10
                            span.duration /mo
                          ul.pricing-content
                            li 50GB Disk Space
                            li 50 Email Accounts
                            li Maintenance
                            li 15 Subdomains
                          .pricingtable-signup
                            a.btn.btn-primary.btn-lg.inline-block.text-white.rounded-lg(href='#' class="hover:text-white") Sign Up
                      div(class="col-span-3 lg:col-span-6 sm:col-span-12")
                        .pricingtable
                          .pricingtable-header
                            h3.title Premium
                          .price-value
                            span.currency $
                            span.amount 20
                            span.duration /mo
                          ul.pricing-content
                            li 10% on all product
                            li 50 Email Accounts
                            li Maintenance
                            li 15 Subdomains
                          .pricingtable-signup
                            a.btn.btn-primary.btn-lg.inline-block.text-white.rounded-lg(href='#' class="hover:text-white") Sign Up
                      div(class="col-span-3 lg:col-span-6 sm:col-span-12")
                        .pricingtable
                          .pricingtable-header
                            h3.title Business
                          .price-value
                            span.currency $
                            span.amount 50
                            span.duration /mo
                          ul.pricing-content
                            li Upload 50 product
                            li 50 Email Accounts
                            li Maintenance
                            li 15 Subdomains
                          .pricingtable-signup
                            a.btn.btn-primary.btn-lg.inline-block.text-white.rounded-lg(href='#' class="hover:text-white") Sign Up
                      div(class="col-span-3 lg:col-span-6 sm:col-span-12")
                        .pricingtable
                          .pricingtable-header
                            h3.title Author pack
                          .price-value
                            span.currency $
                            span.amount 75
                            span.duration /mo
                          ul.pricing-content
                            li Upload 50 product
                            li 50 Email Accounts
                            li Maintenance
                            li 15 Subdomains
                          .pricingtable-signup
                            a.btn.btn-primary.btn-lg.inline-block.text-white.rounded-lg(href='#' class="hover:text-white") Sign Up
                .card
                  .card-header 
                    h5 Simple Pricing Card
                  .card-body.pricing-content
                    .grid.grid-cols-12.gap-4
                      div(class="col-span-3 xxl:col-span-6 sm:col-span-12 box-col-6")
                        .card.text-center.pricing-simple.h-full
                          .card-body
                            h4 Standard
                            h5 $15
                            ul.pricing-content
                              li 
                                i.fa-solid.fa-circle-check
                                | No setup costs
                              li 
                                i.fa-solid.fa-circle-check
                                | Hotline Support for 24/7
                              li 
                                i.fa-solid.fa-circle-check
                                | Priority Support
                              li 
                                i.fa-solid.fa-circle-check
                                | Key Components
                          a.btn.btn-primary.btn-lg.block.text-white.rounded-lg(href='#' class="hover:text-white") Purchase
                      div(class="col-span-3 xxl:col-span-6 sm:col-span-12 box-col-6")
                        .card.text-center.pricing-simple.h-full
                          .card-body
                            h4 Business
                            h5 $25
                            ul.pricing-content
                              li 
                                i.fa-solid.fa-circle-check
                                | 40+ Demos
                              li 
                                i.fa-solid.fa-circle-check
                                | Customer Supports
                              li 
                                i.fa-solid.fa-circle-check
                                | Email Supports
                              li 
                                i.fa-solid.fa-circle-check
                                | Basic Analytics
                          a.btn.btn-primary.btn-lg.block.text-white.rounded-lg(href='#' class="hover:text-white") Purchase
                      div(class="col-span-3 xxl:col-span-6 sm:col-span-12 box-col-6")
                        .card.text-center.pricing-simple.h-full
                          .card-body
                            h4 Premium
                            h5 $35
                            ul.pricing-content
                              li 
                                i.fa-solid.fa-circle-check
                                | Priority Email Supports
                              li 
                                i.fa-solid.fa-circle-check
                                | Customizable Dashboards
                              li 
                                i.fa-solid.fa-circle-check
                                | Advanced Analytics
                              li 
                                i.fa-solid.fa-circle-check
                                | All Basic Features
                          a.btn.btn-primary.btn-lg.block.text-white.rounded-lg(href='#' class="hover:text-white") Purchase
                      div(class="col-span-3 xxl:col-span-6 sm:col-span-12 box-col-6")
                        .card.text-center.pricing-simple.h-full
                          .card-body
                            h4 Extra
                            h5 $45
                            ul.pricing-content
                              li 
                                i.fa-solid.fa-circle-check
                                | Customized Onboarding and Training
                              li 
                                i.fa-solid.fa-circle-check
                                | Hotline Support for 24/7
                              li 
                                i.fa-solid.fa-circle-check
                                | Access to all templates
                              li 
                                i.fa-solid.fa-circle-check
                                | Priority email support
                          a.btn.btn-primary.btn-lg.block.text-white.rounded-lg(href='#' class="hover:text-white") Purchase
        include ../../components/footer
    include ../../components/footer-files
