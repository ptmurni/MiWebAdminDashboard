- var page_Title = 'Masonry Gallery';
- var masonry_js            = true;
- var theme_customizer      = true;
- var sidebar               = true;
- var slick                 = true;
- var header_slider         = true;
- var scrollbar             = true;
doctype html
html(lang='en')
  include ../../components/header-files
  body
    include ../../components/loader
    include ../../components/taptop
    // page-wrapper Start
    #pageWrapper.page-wrapper.compact-wrapper
      include ../../components/header
      // Page Body Start
      .page-body-wrapper
        include ../../components/sidebar
        .page-body
          .container.w-full
            .page-title
              .grid.grid-cols-12.mx-2.items-center
                div(class="col-span-6 sm:col-span-12")
                  h3
                    | Masonry Gallery

                div(class="col-span-6 sm:col-span-12")
                  ol.breadcrumb.flex.gap-2
                    li.breadcrumb-item
                      a(href='index.html')                                        
                        svg.stroke-icon
                          use(href='../assets/svg/icon-sprite.svg#stroke-home')
                    li.breadcrumb-item Gallery
                    li.breadcrumb-item.active Masonry Gallery
          // Container-fluid starts
          .container
            .grid.grid-cols-12
              dic(class="col-span-12 box-col-12")
                .card
                  .card-header
                    h5 MASONRY GALLERY
                  .card-body.product-wrapper-grid
                    .grid.grid-cols-12.card-gap.masonryComponent
                      figure(itemprop='associatedMedia' class="col-span-3 md:col-span-6 sm:col-span-12 masonryItem")
                        a(href='#', itemprop='contentUrl', data-size='1600x950')
                          img.img-thumbnail(src='../assets/images/masonry/1.jpg', itemprop='thumbnail', alt='Image description')
